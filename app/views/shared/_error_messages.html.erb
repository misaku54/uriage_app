<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger" role="alert">
      エラーが<%= object.errors.count %>個あります。修正してください。
    </div>
    <ul>
    <% object.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>