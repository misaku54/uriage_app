<div class="well">
  <%= search_form_for q, url: url, class: "mb-5" do |f| %>
    <div class="form_group row">
      <%= f.label :name_cont, label, class: "col-sm-2 col-form-label"%>
      <div class="col-sm-10">
        <%= f.search_field :name_cont, class: "form-control" %>
      </div>
    </div>
    <div class="form_group">
      <%= f.submit '検索', class: "btn btn-outline-primary" %>
    </div>
  <% end %>
</div>